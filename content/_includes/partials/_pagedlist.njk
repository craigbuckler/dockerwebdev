{% if pagedlist %}
<nav class="pagedlist">

  <ul>

    {%- for post in pagedlist -%}
      <li><a href="{{ post.url | url }}">

        <h2>{{ post.data.title }}</h2>

        {% if dateshow and post.data.date %}<p class="time"><time datetime="{{ post.data.date | dateymd }}">{{ post.data.date | datefriendly }}</time></p>{% endif %}

        <p>{{ post.data.description }}</p>

      </a></li>
    {%- endfor -%}

  </ul>

</nav>
{% endif %}
